###########################################################################
# 2025-06-25  NSS 满血 & 零冲突 —— 调试符号**全开**
###########################################################################

# 1. 关闭冲突 offload（不变）
CONFIG_PACKAGE_kmod-nf-flow=n
CONFIG_PACKAGE_kmod-ipt-offload=n
CONFIG_PACKAGE_kmod-fast-classifier=n
CONFIG_PACKAGE_kmod-shortcut-fe=n
CONFIG_PACKAGE_kmod-shortcut-fe-cm=n
CONFIG_PACKAGE_sqm-scripts=n
CONFIG_PACKAGE_kmod-sched-cake=n
CONFIG_KERNEL_MPTCP=n
CONFIG_KERNEL_NETKIT=n

# 2. 打开 NSS 硬件加速/多核符号（不变）
CONFIG_PACKAGE_kmod-qca-ssdk-nohnat=y
CONFIG_PACKAGE_kmod-qca-rfs=y
CONFIG_PACKAGE_kmod-qca-pppe=y
CONFIG_PACKAGE_kmod-qca-nss-drv-shaper=y
CONFIG_PACKAGE_kmod-qca-nss-drv-igs=y
CONFIG_PACKAGE_kmod-qca-nss-drv-crypto=y
CONFIG_PACKAGE_kmod-qca-nss-crypto=y
CONFIG_PACKAGE_kmod-nss-ifb=y
CONFIG_PACKAGE_MAC80211_NSS_REDIRECT=y
CONFIG_KERNEL_RPS=y
CONFIG_KERNEL_RFS=y
CONFIG_KERNEL_XPS=y
CONFIG_KERNEL_SKB_RECYCLER_MULTI_CPU=y
CONFIG_KERNEL_SKB_RECYCLER_PREALLOC=y
CONFIG_KERNEL_HZ_1000=y
CONFIG_KERNEL_PREEMPT=y
CONFIG_KERNEL_CGROUPS=y
CONFIG_KERNEL_CGROUP_SCHED=y
CONFIG_KERNEL_FAIR_GROUP_SCHED=y
CONFIG_KERNEL_CFS_BANDWIDTH=y
CONFIG_KERNEL_NET_L3_MASTER_DEV=y
CONFIG_NSS_MEM_PROFILE_HIGH=y

# 3. 调试符号**全开**（与 NSS 无冲突）
CONFIG_KERNEL_DEBUG_FS=y
CONFIG_KERNEL_DEBUG_KERNEL=y
CONFIG_KERNEL_PRINTK_TIME=y
CONFIG_KERNEL_DEBUG_INFO=y
CONFIG_KERNEL_DEBUG_INFO_REDUCED=n          # 全量调试
CONFIG_KERNEL_FTRACE=y
CONFIG_KERNEL_FUNCTION_TRACER=y
CONFIG_KERNEL_FUNCTION_GRAPH_TRACER=y
CONFIG_KERNEL_DYNAMIC_FTRACE=y
CONFIG_KERNEL_FTRACE_MCOUNT_RECORD=y
CONFIG_KERNEL_TRACING=y
CONFIG_KERNEL_KPROBES=y
CONFIG_KERNEL_KPROBE_EVENTS=y
CONFIG_KERNEL_BPF_EVENTS=y
CONFIG_KERNEL_PERF_EVENTS=y
CONFIG_KERNEL_CGROUP_BPF=y

# 4. 保留常用 debug 节点（可选）
CONFIG_PACKAGE_kmod-qca-nss-drv-debug=y
CONFIG_PACKAGE_kmod-qca-nss-ecm-debug=y
